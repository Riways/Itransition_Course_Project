{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "totten_romatoes.Client": {
        "Profile": "IdentityServerSPA"
      }
    },
    "Key": {
      "Type": "Development"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+"
      },
      "Https": {
        "Url": "https://+"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/https/certificate_fullchain.pfx",
        "Password": "password"
      }
    }
  },
  "AllowedHosts": "*"
}
